<Window x:Class="PasswordManager.CreateFile"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PasswordManager"
        mc:Ignorable="d"
        Title="CreateFile" Height="450" Width="800">
    <Grid RenderTransformOrigin="0.5,0.5">
        
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="20" Margin="185,92,0,0" TextWrapping="Wrap" Text="Password" VerticalAlignment="Top" Width="215"/>
        <Button x:Name="button" Content="Create" HorizontalAlignment="Left" Height="20" Margin="416,92,0,0" VerticalAlignment="Top" Width="85" Click="button_Click"/>
        <Label x:Name="PasswordLabel" Content="Password:" HorizontalAlignment="Left" Height="23" Margin="184,71,0,0" VerticalAlignment="Top" Width="107"/>
        <Label x:Name="label" Content="Create New File" HorizontalAlignment="Left" Height="42" Margin="163,14,0,0" VerticalAlignment="Top" Width="438" HorizontalContentAlignment="Center" FontWeight="Bold" FontSize="24"/>

    </Grid>
</Window>
