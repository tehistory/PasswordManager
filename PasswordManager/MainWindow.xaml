<Window x:Class="PasswordManager.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PasswordManager"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <DockPanel LastChildFill="False">
            <StackPanel Width="200" >
                <Rectangle Height="40" Stroke="White" Width="182"/>
                <Button x:Name="Createbutton" Content="Create New Username/Password" Height="30" Width="180"/>
                <Rectangle Height="100" Stroke="White" Width="182"/>
                <Button x:Name="Deletebutton" Content="DeleteFile" Height="30" Width="180"/>
                <Rectangle Height="100" Stroke="White" Width="182"/>
                <Button x:Name="Exportbutton" Content="Export/Import Passwords" Height="30" Width="180"/>
            </StackPanel>
            <StackPanel Width="600"/>
        </DockPanel>

    </Grid>
</Window>
